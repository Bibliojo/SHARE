OrderedDict([(u'id', u'http://arxiv.org/abs/0811.3325v1'), (u'updated', u'2008-11-20T12:28:46Z'), (u'published', u'2008-11-20T12:28:46Z'), (u'title', u'On mappings of terms determined by hypersubstitutions'), (u'summary', u'The extensions of hypersubstitutions are mappings on the set of all terms. In\nthe present paper we characterize all hypersubstitutions which provide\nbijections on the set of all terms. The set of all such hypersubstitutions\nforms a monoid. On the other hand, one can modify each hypersubstitution to any\nmapping on the set of terms. For this we can consider mappings from the set of\nall hypersubstitutions into the set of all mappings on the set of all terms.'), (u'author', [OrderedDict([(u'name', u'Jorg Koppitz')]), OrderedDict([(u'name', u'Slavcho Shtrakov')])]), (u'arxiv:comment', OrderedDict([(u'@xmlns:arxiv', u'http://arxiv.org/schemas/atom'), ('#text', u'10 pages, Journal "Algebra and Discrete Mathematics", Number 3\n  (2005), pp. 18-29')])), (u'link', [OrderedDict([(u'@href', u'http://arxiv.org/abs/0811.3325v1'), (u'@rel', u'alternate'), (u'@type', u'text/html')]), OrderedDict([(u'@title', u'pdf'), (u'@href', u'http://arxiv.org/pdf/0811.3325v1'), (u'@rel', u'related'), (u'@type', u'application/pdf')])]), (u'arxiv:primary_category', OrderedDict([(u'@xmlns:arxiv', u'http://arxiv.org/schemas/atom'), (u'@term', u'math.RA'), (u'@scheme', u'http://arxiv.org/schemas/atom')])), (u'category', [OrderedDict([(u'@term', u'math.RA'), (u'@scheme', u'http://arxiv.org/schemas/atom')]), OrderedDict([(u'@term', u'20M14; 20M07'), (u'@scheme', u'http://arxiv.org/schemas/atom')])])])